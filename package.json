{
  "name": "profitti-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./bin/www",
    "dev": "DEBUG=profitti-backend:* babel-watch ./bin/www",
    "test": "BABEL_ENV=test ./mocha.runner.sh",
    "mocha": "BABEL_ENV=test ./node_modules/.bin/mocha --require test-setup.js --compilers js:babel-core/register $(find . -path ./node_modules -prune -o -name '*.spec.js')",
    "lint": "./node_modules/.bin/eslint -c .eslintrc **/*.js",
    "test-all": "npm run lint && npm run mocha"
  },
  "author": "Teemu Tiilikainen",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.24.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "1.17.1",
    "connect-ensure-login": "0.1.1",
    "cookie-parser": "1.4.3",
    "debug": "2.6.3",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "express-session": "1.15.2",
    "knex": "0.12.9",
    "morgan": "1.8.1",
    "objection": "0.7.12",
    "passport": "0.3.2",
    "passport-auth0": "0.6.0",
    "pg": "6.1.5",
    "pug": "2.0.0-beta11",
    "serve-favicon": "2.4.2"
  },
  "devDependencies": {
    "babel-eslint": "7.2.2",
    "babel-plugin-rewire": "1.1.0",
    "babel-watch": "2.0.6",
    "chai": "3.5.0",
    "eslint": "3.19.0",
    "eslint-config-semistandard": "8.0.0",
    "eslint-config-standard": "7.1.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-node": "4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "mocha": "3.2.0",
    "sinon": "2.1.0",
    "sinon-chai": "2.9.0",
    "sinon-stub-promise": "4.0.0"
  },
  "engines": {
    "node": "7.8.0",
    "npm": "4.2.0"
  }
